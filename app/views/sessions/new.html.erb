<div class="container text-center">

	<p><%= link_to image_tag("login_with_facebook.png", height: "100px"), "/auth/facebook", id: "facebook-login"  %></p>

	<h1>Log In with Email</h1>
	<%= form_for :session do |f| %>
		<p><%= f.label :email, class: 'user-signin-form-label' %>
		<%= f.text_field :email %><p>
		<p><%= f.label :password, class: 'user-signin-form-label' %>
		<%= f.text_field :password %><p>
		<p><%= f.submit "Submit" %></p>
	<% end %>

</div>