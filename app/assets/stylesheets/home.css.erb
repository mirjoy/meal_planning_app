#hero-image {
  background: url("<%= asset_path("front-page-oats.jpg")%>");
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
  background-attachment: fixed;
}

#front-page-signup {
	padding-top: 150px;
	padding-bottom: 120px;
	text-align: center;
}

.front-page-text {
	color: #ffffff;
}

h1.front-page-text {
	font-size: 5em;
	font-family: 'Lobster', cursive !important;
}

#front-page-tagline {
	padding-bottom: 50px;
}

.front-page-description {
	padding: 30px;
}

.front-page-image {
	padding-bottom: 10px;
	text-align: center;
	vertical-align: center;
}

.front-row {
	background-color: #D2A176;
}

#middle-image{
	text-align: center;
}

#front-page-input {
	padding: 20px;
}

#submit-btn{
	margin-top: 10px;
}

